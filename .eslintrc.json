{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": 
    {
        "prettier/prettier": [
          "error",
          {
              "endOfLine":"auto",
            "semi": false,
            "singleQuote": false,
            "tabWidth": 2,
            "trailingComma": "es5"
          }
        ],
        "react/jsx-filename-extension": [
          "error",
          {
            "extensions": [
                ".js",
                ".jsx"
            ]
          }
        ],
        "react/prop-types": 0,
        "no-unused-vars": [
          "error",
          {
            "vars": "local",
            "args": "none"
          }
        ]
        
      }
}
